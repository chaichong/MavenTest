<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cc.dao.UserDAO">
    
    <select id="getUserEntityByAccount" resultType="cn.cc.entity.User" parameterType="cn.cc.entity.User">
        SELECT * FROM user WHERE userAccount = ${userAccount}
    </select>
    
</mapper>